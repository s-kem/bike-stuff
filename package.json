{
  "name": "gear-ratio-ui",
  "version": "0.1.0",
  "description": "Web app to show gear bicycle gear ratios",
  "keywords": [
    "react",
    "redux",
    "ui",
    "redux-saga",
    "jest",
    "react-router"
  ],
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run build:prod && npm run build:server && npm run build:deploy && npm run build:pkg",
    "build:prod": "NODE_ENV=production webpack",
    "build:dev": "NODE_ENV=development webpack",
    "build:server": "cp src/server/server.js dist",
    "build:deploy": "cp package.json dist",
    "build:pkg": "mv node_modules dist",
    "clean": "rm -rf dist/",
    "dev": "NODE_ENV=development node src/server/server.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --fix --format table --ext .js,.jsx src --cache",
    "lint:css": "stylelint \"src/client/styles/**/*.css\" --fix --color --allow-empty-input --cache",
    "lint:clean": "eslint --report-unused-disable-directives --ext .js,.jsx src",
    "preversion": "npm test && npm run lint",
    "postversion": "git push --tags internal HEAD:master",
    "test": "BABEL_ENV=test jest",
    "test:chain": "BABEL_ENV=test jest --runInBand",
    "test:coverage": "BABEL_ENV=test jest --coverage",
    "test:watch": "BABEL_ENV=test jest --watch",
    "start": "NODE_ENV=production node server.js"
  },
  "author": "Sean Kemmis",
  "repository": {
    "type": "git",
    "url": "https://github.platforms.engineering/s-kem"
  },
  "engines": {
    "node": "8.9.x"
  },
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.2",
    "react-select": "^1.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "compression-webpack-plugin": "^1.1.2",
    "css-loader": "^0.28.5",
    "eslint": "^4.14.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-wyze": "^3.3.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "material-ui": "^0.20.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "style-loader": "^0.19.1",
    "stylelint": "^8.4.0",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.1",
    "whatwg-fetch": "^2.0.3"
  }
}
